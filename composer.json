{
    "name": "chang/erp-nova-components",
    "description": "Erp System Nova components.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Chang\\Erp\\Nova\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chang\\Erp\\Nova\\ErpNovaComponentServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
